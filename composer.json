{
    "name": "slimphp-api/slim-phinx",
    "description": "A module for slim-api for phinx migration, integrating migrations for slim-api",
    "keywords": ["slim","api","phinx","slimapi"],
    "require": {
        "php": ">5.6",
        "robmorgan/phinx": "0.*",
        "slimphp-api/slim-api": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mikey179/vfsStream": "1.*",
        "satooshi/php-coveralls": "^0.7.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Baker",
            "email": "me@g403.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"SlimApi\\Phinx\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"SlimApi\\PhinxTest\\": "tests/phpunit/"}
    }
}
